msgid "Play"
msgstr "Odtwórz"

#. Crew group name for movie cast
msgctxt "CREW_CAST"
msgid "Cast"
msgstr "Obsada"

#. Crew group name for movie directors
msgctxt "CREW_DIRECTORS"
msgid "Directors"
msgstr "Reżyseria"

#. Crew group name for movie writers
msgctxt "CREW_WRITERS"
msgid "Writers"
msgstr "Scenariusz"

#. Escaping button on exit dialog
msgctxt "EXIT_CANCEL"
msgid "Cancel"
msgstr "Anuluj"

#. Header for exiting the app on systems with confired exit
msgctxt "EXIT_MESSAGE"
msgid "Do you want to close the application?"
msgstr "Czy chcesz zamknąć aplikację?"

#. Actionable button on exit dialog
msgctxt "EXIT_OK"
msgid "Exit"
msgstr "Wyjdź"

#. Section title for episode list
msgctxt "HEADER_EPISODES"
msgid "Episodes"
msgstr "Odcinki"

#. Section title for a list of extra features
msgctxt "HEADER_EXTRAS"
msgid "Extra features"
msgstr "Dodatkowe materiały"

#. Section title for episode list
msgctxt "HEADER_GALLERY"
msgid "Gallery"
msgstr "Galeria"

#. Header for listing views waiting for items
msgctxt "LOADING_HEADER"
msgid "Loading..."
msgstr "Ładowanie..."

#. Hint if some filters, but no search terms, are applied
msgctxt "NO_ITEMS_CHANGE_FILTERS"
msgid "Maybe different filters would show more results."
msgstr "Może inne filtry pokazałyby więcej wyników."

#. Hint if both some filters and search terms are applied
msgctxt "NO_ITEMS_CHANGE_FILTERS_OR_SEARCH"
msgid "Maybe different filters or search terms would show more results."
msgstr "Być może inne filtry lub wyszukiwane hasła pokażą więcej wyników."

#. Hint if some search terms, but no filters, are applied
msgctxt "NO_ITEMS_CHANGE_SEARCH"
msgid "Maybe different search terms would show more results."
msgstr "Być może inne wyszukiwane hasła wyświetlą więcej wyników."

#. Hint if data websocket is disconnected
msgctxt "NO_ITEMS_DISCONNECTED"
msgid "There is no connection to database."
msgstr "Nie ma połączenia z bazą danych."

#. Header for list of currently active filters
msgctxt "NO_ITEMS_FILTERS_HEADER"
msgid "Currently selected:"
msgstr "Aktualnie wybrane:"

#. Empty listing header
msgctxt "NO_ITEMS_HEADER"
msgid "No items"
msgstr "Brak elementów"
