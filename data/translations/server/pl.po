msgid "Has IMDb link"
msgstr "Posiada link do IMDb"

#. Filter label for movies not matching 'has_imdb'
msgctxt "FILTER_LABEL_HAS_IMDB_OPOSITE"
msgid "Has no IMDb link"
msgstr "Nie posiada linku do IMDb"

#. Filter label for movies with detailed descriptionm
msgctxt "FILTER_LABEL_HAS_INFO"
msgid "Has info"
msgstr "Posiada informacje"

#. Filter label for movies not matching 'has_info'
msgctxt "FILTER_LABEL_HAS_INFO_OPOSITE"
msgid "Has no info"
msgstr "Nie posiada informacji"

#. Filter label for movies with local content
msgctxt "FILTER_LABEL_HAS_VIDEO"
msgid "Has video"
msgstr "Zawiera wideo"

#. Filter label for movies not matching 'has_video'
msgctxt "FILTER_LABEL_HAS_VIDEO_OPOSITE"
msgid "Has no video"
msgstr "Nie zawiera widea"

#. Filter label for titles with episodes
msgctxt "FILTER_LABEL_IS_SERIES"
msgid "Is series"
msgstr "Jest serialem"

#. Filter label for titles without episodes
msgctxt "FILTER_LABEL_IS_SERIES_OPOSITE"
msgid "Is not series"
msgstr "Nie jest serialem"

#. Filter label for number of stars
msgctxt "FILTER_LABEL_RATING"
msgid "Rating"
msgstr "Ocena"

#. Filter label for length in minutes
msgctxt "FILTER_LABEL_RUNTIME"
msgid "Run time"
msgstr "Czas wykonywania"

#. Filter label for tags associated with te movie
msgctxt "FILTER_LABEL_TAGS"
msgid "Tags"
msgstr "Tagi"

#. Filter label for release year
msgctxt "FILTER_LABEL_YEAR"
msgid "Release year"
msgstr "Rok wydania"

#. Group label for items ordered by arrival time, for items in the future
msgctxt "GROUP_LABEL_FUTURE"
msgid "Soon"
msgstr "Wkrótce"

#. Group label for items ordered by arrival time, for items in the previous month
msgctxt "GROUP_LABEL_LAST_MONTH"
msgid "Previous month"
msgstr "Poprzedni miesiąc"

#. Group label for items ordered by arrival time, for items in the previous year
msgctxt "GROUP_LABEL_LAST_YEAR"
msgid "Previous year"
msgstr "Ubiegły rok"

#. Group label for items ordered by arrival time, for items older, than previous year
msgctxt "GROUP_LABEL_LONG_AGO"
msgid "Older"
msgstr "Starsze"

#. Group label for items ordered by arrival time, for items since the begining of this month
msgctxt "GROUP_LABEL_THIS_MONTH"
msgid "This month"
msgstr "Ten miesiąc"

#. Group label for items ordered by arrival time, for items since the begining of this year
msgctxt "GROUP_LABEL_THIS_YEAR"
msgid "This year"
msgstr "Ten rok"

#. Sort label for order by arrival to database
msgctxt "SORT_LABEL_ARRIVAL"
msgid "Modification date"
msgstr "Data modyfikacji"

#. Sort label for order by number of stars
msgctxt "SORT_LABEL_RATING"
msgid "Rating"
msgstr "Ocena"

#. Sort label for order by length in minutes
msgctxt "SORT_LABEL_RUNTIME"
msgid "Run time"
msgstr "Czas wykonywania"

#. Sort label for alfabetical order
msgctxt "SORT_LABEL_TITLE"
msgid "Title"
msgstr "Tytuł"

#. Sort label for order by movie release year
msgctxt "SORT_LABEL_YEAR"
msgid "Release year"
msgstr "Rok wydania"
