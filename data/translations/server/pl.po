#. Universal error message
msgctxt "ARGS_ARGUMENT_MSG"
msgid "argument {0}: {1}"
msgstr "argument {0}: {1}"

#. Default name of an argument
msgctxt "ARGS_DEF_META"
msgid "<arg>"
msgstr "<arg>"

#. Name of a directory argument
msgctxt "ARGS_DIR_META"
msgid "<dir>"
msgstr "<katalog>"

#. Error message template; placeholder 0 will get the name of program, placeholder 1 an actual message
msgctxt "ARGS_ERROR_MSG"
msgid "{0}: error: {1}"
msgstr "{0}: błąd: {1}"

#. Error message for conflicting arguments; used with ARGUMENT_MSG
msgctxt "ARGS_EXCLUSIVE"
msgid "not allowed with argument {0}"
msgstr "nie można używać z {0}"

#. Name of a file argument
msgctxt "ARGS_FILE_META"
msgid "<file>"
msgstr "<plik>"

#. Needed file was not found
msgctxt "ARGS_FILE_NOT_FOUND"
msgid "cannot open {0}"
msgstr "nie można otworzyć {0}"

#. Description for the -h/--help argument
msgctxt "ARGS_HELP_DESCRIPTION"
msgid "shows this help message and exits"
msgstr "pokazuje ten komunikat pomocy i wychodzi"

#. Name of a, well, name argument, e.g. in cov config
msgctxt "ARGS_NAME_META"
msgid "<name>"
msgstr "<nazwa>"

#. Error message for a numeric argument, when parsing would get outside of representable bounds
msgctxt "ARGS_NEEDED_NUMBER_EXCEEDED"
msgid "argument {0}: number outside of expected bounds"
msgstr "argument {0}: liczba poza oczekiwanym zakresem"

#. Help message presenting a list of possible values for a list-type argument
msgctxt "ARGS_NEEDS_ENUM_KNOWN_VALUES"
msgid "known values for {0}: {1}"
msgstr "znane wartości dla {0}: {1}"

#. Error message for a list of arguments, when parsing failed
msgctxt "ARGS_NEEDS_ENUM_UNKNOWN"
msgid "argument {0}: value {1} is not recognized"
msgstr "argument {0}: wartość {1} jest nierozpoznana"

#. Error message for unneeded value
msgctxt "ARGS_NEEDS_NO_PARAM"
msgid "argument {0}: value was not expected"
msgstr "argument {0}: wartość nie była oczekiwana"

#. Error message for a numeric argument, when parsing failed
msgctxt "ARGS_NEEDS_NUMBER"
msgid "argument {0}: expected a number"
msgstr "argument {0}: oczekiwano liczby"

#. Error message for a missing argument
msgctxt "ARGS_NEEDS_PARAM"
msgid "argument {0}: expected one argument"
msgstr "argument {0}: oczekiwano jednego argumentu"

#. Header for list of optional arguments
msgctxt "ARGS_OPTIONALS"
msgid "optional arguments"
msgstr "argumenty opcjonalne"

#. Name of a file or directory argument
msgctxt "ARGS_PATH_META"
msgid "<path>"
msgstr "<ścieżka>"

#. Header for list of positional arguments
msgctxt "ARGS_POSITIONALS"
msgid "positional arguments"
msgstr "argumenty pozycyjne"

#. Error message for a missing required argument
msgctxt "ARGS_REQUIRED"
msgid "argument {0} is required"
msgstr "argument {0} jest wymagany"

#. Error message for an unrecognized argument; the placeholder will contain the name of the argument
msgctxt "ARGS_UNRECOGNIZED"
msgid "unrecognized argument: {0}"
msgstr "nierozpoznany argument: {0}"

#. Synopsis header; please note there is one space at the end of this entry
msgctxt "ARGS_USAGE"
msgid "usage: "
msgstr "użycie: "

#. Name of a value argument, e.g. in cov config
msgctxt "ARGS_VALUE_META"
msgid "<value>"
msgstr "<wartość>"

#. Warning message template; placeholder 0 will get the name of program, placeholder 1 an actual message
msgctxt "ARGS_WARNING_MSG"
msgid "{0}: warning: {1}"
msgstr "{0}: ostrzeżenie: {1}"

#. Filter label for age restriction
msgctxt "FILTER_LABEL_AGE"
msgid "Age rating"
msgstr "Kategoria wiekowa"

#. Filter label for arrival to database
msgctxt "FILTER_LABEL_ARRIVAL"
msgid "Modification date"
msgstr "Data modyfikacji"

#. Filter label for movies with local or downloadable content
msgctxt "FILTER_LABEL_AVAILABLE"
msgid "Available"
msgstr "Dostępne"

#. Filter label for movies not matching 'available'
msgctxt "FILTER_LABEL_AVAILABLE_OPOSITE"
msgid "Not available"
msgstr "Niedostępne"

#. Filter label for countries of origin
msgctxt "FILTER_LABEL_COUNTRIES"
msgid "Countries"
msgstr "Kraje"

#. Filter label for genres
msgctxt "FILTER_LABEL_GENRES"
msgid "Genres"
msgstr "Gatunki"

